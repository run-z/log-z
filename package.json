{
  "name": "@run-z/log-z",
  "version": "1.2.1",
  "description": "Log that in browser or Node.js",
  "keywords": [
    "log",
    "logger",
    "logging"
  ],
  "homepage": "https://github.com/run-z/log-z",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:run-z/log-z.git"
  },
  "license": "MIT",
  "author": "Ruslan Lopatin <ruslan.lopatin@gmail.com>",
  "bugs": {
    "url": "https://github.com/run-z/log-z/issues"
  },
  "type": "module",
  "main": "./dist/log-z.cjs",
  "module": "./dist/log-z.js",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/log-z.js",
      "require": "./dist/log-z.cjs"
    },
    "./node": {
      "import": "./dist/log-z.node.js",
      "require": "./dist/log-z.node.cjs"
    }
  },
  "dependencies": {
    "@proc7ts/primitives": "^1.8.0",
    "@proc7ts/push-iterator": "^2.5.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@run-z/eslint-config": "^1.2.1",
    "@run-z/rollup-helpers": "^1.0.0",
    "@types/fs-extra": "^9.0.7",
    "@types/jest": "^26.0.20",
    "@types/node": "^12.20.4",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "codecov": "^3.8.1",
    "eslint": "^7.20.0",
    "eslint-plugin-jest": "^24.1.5",
    "fs-extra": "^9.1.0",
    "gh-pages": "^3.1.0",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "rollup": "^2.39.1",
    "rollup-plugin-flat-dts": "^1.0.1",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript2": "^0.30.0",
    "run-z": "=1.6.1-bootstrap",
    "shx": "^0.3.3",
    "strip-ansi": "^6.0.0",
    "ts-jest": "^26.5.2",
    "tslib": "^2.1.0",
    "typedoc": "^0.20.28",
    "typescript": "^4.2.2"
  },
  "scripts": {
    "all": "run-z build,lint,test",
    "bootstrap": "rollup --config ./rollup.config.js",
    "build": "run-z +z bootstrap",
    "ci:all": "run-z all +test/--ci/--runInBand",
    "clean": "run-z +z --then shx rm -rf index.d.ts '*/index.d.ts' dist target",
    "doc": "run-z +z --then typedoc",
    "doc:publish": "run-z doc --then gh-pages --dist target/typedoc --dotfiles",
    "lint": "run-z +z --then eslint .",
    "test": "run-z +z --then jest",
    "z": "run-z +bootstrap,+doc,+lint,+test"
  }
}
